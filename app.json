{
  "expo": {
    "name": "ByeSmoke AI - Smart Quit Coach",
    "slug": "byesmoke-v00",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "byesmokev00",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "description": "Smart AI-powered quit smoking coach with personalized guidance, progress tracking, and community support to help you become smoke-free.",
    "primaryColor": "#F99546",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zaynstudio.byesmoke",
      "buildNumber": "1.0.2",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "NSPhotoLibraryUsageDescription": "ByeSmoke needs access to save your progress images to your photo library.",
        "NSPhotoLibraryAddUsageDescription": "ByeSmoke needs permission to save your smoke-free achievement cards.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.zaynstudio.byesmoke",
      "versionCode": 2,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "blockedPermissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#F99546"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-1627952637319380~9785170226",
          "iosAppId": "ca-app-pub-1627952637319380~5193561129"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.161013631866-c4rmolnq9tuhvjihodkthfsjfggn0edu"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow ByeSmoke to save your achievement cards to your photo library.",
          "savePhotosPermission": "Allow ByeSmoke to save images to your photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "9801fdf0-23bb-4c50-a0da-de62cbc37659"
      }
    }
  }
}
